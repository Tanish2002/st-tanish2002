*geometry: 105x29
!Vim*useSchemes:			all
Vim*sgiMode:			true
Vim*useEnhancedFSB:		true
!Vim.foreground:			Black
!Vim.background:			Wheat
!Vim*fontList:			7x13
vim*fontList: unifont

! https://github.com/ardumont/dot-files/blob/master/.Xresources
!Xft.antialias: true
!Xft.hinting: true
!Xft.hintstyle: hintlight
!Xft.rgba: rgb


!! st configuration and color schemes

st.alpha: 0.95
st.cursorColor: #44dd88


